    "test": "run-p --race dev cy:run",
    "lint": "eslint --ext .js,.svelte src/"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "fulmo": "../",
    "polka": "^0.5.2",
    "sirv": "^0.4.0"
