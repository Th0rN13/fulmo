{
  "name": "sapper-boilerplate",
  "description": "Sapper boilerplate with user login\\register, user sessions, dotenv etc",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --basepath fulmo",
